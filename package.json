{
  "name": "@chemican/components",
  "private": true,
  "version": "0.0.17",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./utils": {
      "types": "./src/utils.ts",
      "import": "./src/utils.ts",
      "require": "./src/utils.ts"
    },
    "./tabler-icons": {
      "types": "./src/tabler-icons.ts",
      "import": "./src/tabler-icons.ts",
      "require": "./src/tabler-icons.ts"
    },
    "./dist": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./tailwind-preset.css": "./src/tailwind-preset.css",
    "./tailwind-preset-next.css": "./src/tailwind-preset-next.css",
    "./dist/components.css": "./dist/components.css",
    "./src/assets/fonts/*": "./src/assets/fonts/*",
    "./dist/fonts/*": "./dist/fonts/*"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,json,css}'",
    "tokens:generate": "node ./tools/build-tokens.cjs",
    "release": "release-it",
    "prepare": "husky"
  },
  "dependencies": {
    "@figma/code-connect": "^1.3.3",
    "@tabler/icons-react": "^3.34.1",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-table": "^8.21.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "radix-ui": "^1.1.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.17.0",
    "@storybook/addon-designs": "^10.0.1",
    "@storybook/addon-docs": "^9.0.15",
    "@storybook/react-vite": "^9.0.15",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-better-tailwindcss": "^3.7.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-storybook": "^9.0.15",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "prettier-plugin-classnames": "^0.8.0",
    "prettier-plugin-merge": "^0.8.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "release-it": "^18.1.2",
    "storybook": "^9.0.15",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.5.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
